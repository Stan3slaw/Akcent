<!DOCTYPE html>
<html lang="uk">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Akcent</title>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAT6g1SFRKyjMogql--rDZ9D7Wchj9VT9Y&callback=initMap&libraries=&v=weekly" defer></script>
  <link rel="stylesheet" href="./css/main.css">
  <script src="./js/map.js"></script>
</head>

<body>
  <div class="wrapper">
    <!-- @import "header.html" -->

    <main class="main">

      <section class="block1">
        <div class="main__banner"></div>
        <div class="container">
          <h1 class="main__title title">
            Main page
          </h1>
        </div>
      </section>

      <section class="block2">
        <div class="slider__catalogue">
          <div class="slider__screen__catalogue">
            <img src="img/slides_catalogue/01.jpg" alt="">
          </div>
          <div class="slider__screen__catalogue">
            <img src="img/slides_catalogue/02.jpg" alt="">
          </div>
          <div class="slider__screen__catalogue">
            <img src="img/slides_catalogue/03.jpg" alt="">
          </div>
          <div class="slider__screen__catalogue">
            <img src="img/slides_catalogue/04.jpg" alt="">
          </div>
          <div class="slider__screen__catalogue">
            <img src="img/slides_catalogue/05.jpg" alt="">
          </div>
        </div>

        <div class="slider__nav">
          <ul class="slider__menu">
            <li class="slider__nav-item" slideTo="1">
              <div class="highlight"></div>
              <img src="img/svg/svg-catalogue-nav/beer-glass.svg" alt="">
              <p>Пиво</p>
            </li>
            <li class="slider__nav-item" slideTo="2">
              <img src="img/svg/svg-catalogue-nav/wine-bottle.svg" alt="">
              <p>Вино</p>
            </li>
            <li class="slider__nav-item" slideTo="3">
              <img src="img/svg/svg-catalogue-nav/vodka-bottle.svg" alt="">
              <p>Горілка</p>
            </li>
            <li class="slider__nav-item" slideTo="4">
              <img src="img/svg/svg-catalogue-nav/whiskey-bottle.svg" alt="">
              <p>Віскі</p>
            </li>
            <li class="slider__nav-item" slideTo="5">
              <img src="img/svg/svg-catalogue-nav/kognac-bottle.svg" alt="">
              <p>Коньяк</p>
            </li>
          </ul>
        </div>
      </section>

      <section class="block3">
        <div class="partners">Наші Партнери</div>
        <div class="slider__manufacturers">
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/01.png" alt="">
          </div>
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/02.png" alt="">
          </div>
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/03.png" alt="">
          </div>
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/04.png" alt="">
          </div>
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/04.png" alt="">
            <p>5</p>
          </div>
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/01.png" alt="">
          </div>
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/02.png" alt="">
          </div>
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/03.png" alt="">
          </div>
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/04.png" alt="">
          </div>
          <div class="slider__screen__manufacturers">
            <img src="img/slides_manufacturers/04.png" alt="">
          </div>
        </div>
      </section>


    </main>


    <!-- @import "footer.html" -->
  </div>

  <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
  <!-- <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.2/jquery.min.js"></script> -->
  <script src="./js/slick.min.js"></script>
  <script src="./js/script.js"></script>
</body>

</html>